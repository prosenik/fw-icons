{
  "name": "@FDISK-next/FDISK-icon-library",
  "version": "0.0.0",
  "description": "FDISK Design System -  icons library",
  "keywords": [
    "design-system",
    "ui-kit",
    "libraries",
    "icons"
  ],
  "license": "Apache-2.0",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "shx rm -rf dist",
    "build": "npm run build:svg && npm run build:font && npm run build:sprite",
    "build:font": "shx rm -rf ./dist/iconLibrary/font && shx mkdir ./dist/iconLibrary/font && fantasticon",
    "build:sprite": "shx rm -rf ./dist/iconLibrary/sprite && shx mkdir ./dist/iconLibrary/sprite && spritesh --viewbox \"0 0 16 16\" -i dist/iconLibrary/svg -o dist/iconLibrary/sprite/iconLibrary.svg && node src/scripts/add-sprite-declaration.js",
    "build:svg": "node src/scripts/build-svg.js && svgo -f dist/iconLibrary/svg",
    "prestart": "npm run build",
    "start": "concurrently \"chokidar \\\"src/iconLibrary/**/*.svg\\\" -c \\\"npm run build\\\"\" \"node preview-server.js\"",
    "preview": "node preview-server.js"
  },
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "babel-loader": "^8.0.0",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.2.2",
    "express": "^4.18.2",
    "fantasticon": "^1.1.1",
    "fs-extra": "^10.0.0",
    "react": "^18.0.2",
    "react-dom": "^18.0.2",
    "shx": "^0.3.4",
    "spritesh": "^1.1.0",
    "svgo": "^2.7.0",
    "webpack": "4.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ]
}
